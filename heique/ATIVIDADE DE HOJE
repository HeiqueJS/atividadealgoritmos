const leitor=require("readline-sync")

let a= leitor.question("Qual a sua idade?")
let b= leitor.question("Qual a idade de seu melhor amigo?")
let c= leitor.question("Você é mais velho que seu amigo?Se sim digite True se não False.")


console.log(a||b);
console.log(b||c);
console.log(a||c);
console.log(a||b||c);